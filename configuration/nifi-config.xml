<?xml version="1.0"?>
<!-- xml文件内的大部分属性会在ambari web UI上的configs标签页展示，内容可编辑修改 -->
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
<!-- supports_adding_forbidden设置为true，取消自定义配置 -->
<configuration supports_adding_forbidden="true">

    <property>
        <name>server_port</name>
        <displayName>server_port</displayName>
        <value>8443</value>
        <description>Nifi服务端口</description>
        <value-attributes>
            <overridable>false</overridable>
        </value-attributes>
    </property>

    <property>
        <name>node_port</name>
        <displayName>node_port</displayName>
        <value>16342</value>
        <description>Nifi集群协议端口</description>
        <value-attributes>
            <overridable>false</overridable>
        </value-attributes>
    </property>

    <property>
        <name>balance_port</name>
        <displayName>balance_port</displayName>
        <value>6342</value>
        <description>Nifi集群负载均衡端口</description>
        <value-attributes>
            <overridable>false</overridable>
        </value-attributes>
    </property>

</configuration>